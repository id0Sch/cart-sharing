{
  "name": "10bis cart sharing",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension allows you to share your cart with friends",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "/config.js",
      "js/socket.io.js",
      "js/socketiop2p.min.js",
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "notifications",
    "cookies",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.10bis.co.il/*"
      ],
      "js": [
        "js/jquery.js",
        "js/firebase.js",
        "js/dataChannel.js",
        "src/inject/inject.js"
      ],
      "css":
      [
        "src/inject/inject.css"
      ]
    }
  ]
, "web_accessible_resources": [
  "src/inject/inject.css"
]
}